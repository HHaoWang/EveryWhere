{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.EntityFrameworkCore": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "appId": "",
  "appSecret": "",
  "ConnectionStrings": {
    "EveryWhereContext": "server = 127.0.0.1; uid = EveryWhere; pwd = EveryWhere; database = every_where"
  },
  "tokenConfig": {
    "saltValue": "",
    "issuer": "test.cn",
    "audience": "test",
    "accessExpiration": 72,
    "refreshExpiration": 60
  },
  "FileSizeLimit": 31457280,
  "MessageQueue": {
    "RabbitConnect": {
      "HostName": "",
      "Port": 5672,
      "UserName": "",
      "Password": ""
    },
    "BatchSendMessageQueue": {
      "Exchange": "BatchSendMessage",
      "Queue": "BatchSendMessage",
      "ExchangeType": "direct",
      "RouteKey": "BatchSendMessage"
    }
  }
}
